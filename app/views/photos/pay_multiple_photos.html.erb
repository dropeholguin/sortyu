<div id="pay-photos" class="upper-pad under-pad row">
	<div class="row">
		<div class="small-12 text-center columns">
			<%= button_tag('Pay Photos', class:"button delete", id: "pay-multiple-photos") %>
		</div>
	</div>
	<div class="row">
		<% @photos.each do |photo| %>
			<div class="upper-space small-4 text-center end columns">
				<%= image_tag(photo.file.url(:medium),id: photo.id) %>
				<%= check_box_tag('pay_photo', photo.id , false) %>
			</div>
		<% end %>
	</div>
</div>