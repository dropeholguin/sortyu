<% if @photo.first_edit %>
	setTimeout(function(){
		window.LocalTracker.performTracking();
	}, 1000);
<% else %>
	<% @photo.sections.each do |section| %>
		window.LocalPloter.plotSectionEdit(<%= section.left %>, <%= section.top %>, <%= section.width %>,<%= section.height %>,<%= section.translateX %>,<%= section.translateY %>)
	<% end %>
<% end %>