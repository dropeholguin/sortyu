<div class="row">
	<div id="photo-editor" class="callout small-12 medium-10 medium-push-1 large-8 large-push-2 text-center columns">
		<h4>Edit Photo</h4>
		<div class="row">
			<div class="under-space small-12 columns">
				<%= image_tag(@photo.file.url(:large), id:"edit-my-sections")%>
			</div>
		</div>
		<%= render 'form', photo: @photo %>
		<%= link_to 'Back', photo_path(@photo), class: "hollow button" %>
	</div>
</div>
